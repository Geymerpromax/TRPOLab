<Window x:Class="Lab9API.Ex1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab9API"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000" >
    <StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
            <Button Content="МГУ" Click="LoadMoscowMap_Click" Width="100" Margin="5"/>
            <Button Content="Эйфелева башня" Click="LoadEiffelTowerMap_Click" Width="150" Margin="5"/>
            <Button Content="Авачинский вулкан" Click="LoadAvachinskyMap_Click" Width="150" Margin="5"/>
            <Button Content="Байконур" Click="LoadBaikonurMap_Click" Width="100" Margin="5"/>
        </StackPanel>
        <Image x:Name="MapImage" Stretch="Uniform" Margin="10" Height="490"/>
    </StackPanel>
</Window>
